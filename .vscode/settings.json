{
  // "sortJSON.orderOverride": [
  //   // 一番上に"sortJSON.orderOverride"を固定する
  //   "sortJSON.orderOverride"
  //   // よく使う設定項目を記述
  // ],
  "atomKeymap.promptV3Features": true,
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll": true
    // "source.organizeImports": true # sortImport遅い
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontSize": 13,
  "editor.formatOnPaste": true, // ペースト時にフォーマット
  "editor.formatOnSave": true,
  "editor.formatOnType": false, // 入力中（改行時）にフォーマット
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.suggestSelection": "first",
  "emmet.triggerExpansionOnTab": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "explorer.confirmDelete": false,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.suggestSmartCommit": false,
  "go.formatTool": "goimports",
  "hardhat.telemetry": true,
  "hediet.vscode-drawio.local-storage": "",
  "jupyter.debugJustMyCode": false,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.extraPaths": [
    "/.pyenv/versions/anaconda3-2020.11/lib/python3.8/site-packages"
  ],
  // "python.linting.flake8Enabled": true,
  "python.autoComplete.addBrackets": true,
  "python.autoComplete.preloadModules": [
    "numpy",
    "pandas",
    "sklean",
    "matplotlib"
  ],
  "python.condaPath": "~/.pyenv/versions/miniforge3/envs/python39",
  "python.formatting.blackArgs": ["--line-length=79"],
  "python.formatting.blackPath": "~/.pyenv/shims/black",
  "python.formatting.provider": "black",
  // "python.pythonPath": "python",
  "python.jediEnabled": false,
  "python.languageServer": "None",
  "python.linting.flake8Args": ["max-line-length=100"],
  "python.linting.flake8CategorySeverity.E": "Warning",
  "python.linting.flake8CategorySeverity.F": "Warning",
  "python.linting.flake8Path": "~/.pyenv/shims/flake8",
  "python.linting.mypyArgs": [
    "--follow-imports=skip",
    "--ignore-missing-imports",
    "--disallow-untyped-defs",
    "--disallow-untyped-calls",
    "--warn-return-any",
    "--no-implicit-optional"
  ],
  "python.linting.mypyCategorySeverity.error": "Information",
  "python.linting.mypyEnabled": true,
  "python.linting.mypyPath": "~/.pyenv/shims/mypy",
  "python.linting.pylintArgs": [
    "--ignored-modules=numpy,cv2,matplotlib",
    "--ignored-classes=numpy,cv2,matplotlib",
    "--extension-pkg-whitelist=numpy,cv2,matplotlib"
  ],
  "python.sortImports.path": "~/.pyenv/shims/isort",
  // "solidity.compileUsingRemoteVersion": "v0.4.26+commit.4563c3fc",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.scrollback": 10000000,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.zoomLevel": 0,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.iconTheme": "vscode-icons",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "newUntitledFile",
  "[html]": {
    "editor.formatOnSave": false
  },
  "terminal.integrated.allowChords": false
}
